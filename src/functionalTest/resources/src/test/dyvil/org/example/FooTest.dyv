package org.example

import org.junit.Assert
import org.junit.Test

class FooTest {
	@Test func getText() {
		let foo = new Foo
		Assert.assertEquals("Hello World", foo.getText())
	}

	@Test func getABC() {
		let bar1 = new Bar1
		Assert.assertEquals("1 A", bar1.getA())
		Assert.assertEquals("1 B", bar1.getB())
		Assert.assertEquals("1 C", bar1.getC())

		let bar2 = new Bar2
		Assert.assertEquals("2 A", bar2.getA())
		Assert.assertEquals("2 B", bar2.getB())
		Assert.assertEquals("2 C", bar2.getC())
	}

	@Test func getDEF() {
		let jav = new Jav
		Assert.assertEquals("D", jav.getD())
		Assert.assertEquals("E", jav.getE())
		Assert.assertEquals("F", jav.getF())
	}
}
